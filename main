#!/bin/bash
#PBS -l nodes=1:ppn=8:dc2,walltime=0:10:00
#PBS -N FSLBET
#PBS -V

if [ $ENV == "IUHPC" ]; then
    module load fsl
fi

export t1=$(jq -r .t1 config.json)
export fthresh=$(jq -r .fthreshold config.json)

cp $t1 t1_orig.nii.gz

bet t1_orig.nii.gz t1.nii.gz -m -f $fthresh

mv t1_mask.nii.gz mask.nii.gz

