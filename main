#!/bin/bash
#PBS -l nodes=1:ppn=8:dc2,walltime=0:10:00
#PBS -N FSLBET
#PBS -V

if [ $ENV == "IUHPC" ]; then
    module load fsl
fi

export t2=$(jq -r .t2 config.json)
export fthresh=$(jq -r .fthreshold config.json)

cp $t2 ./t2_orig.nii.gz

bet t2_orig.nii.gz t2.nii.gz -m -f $fthresh

mv t2_mask.nii.gz mask.nii.gz

